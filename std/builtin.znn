
export import "std/slice.znn";
export import "std/array.znn";
export import "std/concepts.znn";
export import "std/string.znn";

export template <T>
T* copy(T** p) {
  return *p;
}

export bool copy(bool* b) {
  return *b;
}

export int copy(int* b) {
  return *b;
}

export char copy(char* b) {
  return *b;
}

export double copy(double* b) {
  return *b;
}
