
public import "std/slice.znn";
public import "std/array.znn";
public import "std/concepts.znn";
public import "std/string.znn";

public template <T>
T* copy(T** p) {
  return *p;
}

public bool copy(bool* b) {
  return *b;
}

public int copy(int* b) {
  return *b;
}

public char copy(char* b) {
  return *b;
}

public double copy(double* b) {
  return *b;
}
