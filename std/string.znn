

public int size(string* s) {
  embed {
    return s->size();
  }
}

public bool empty(string* s) {
  return s->size() == 0;
}

public string substring(string* s, int start, int length) {
  embed {
    return s->substring(start, length);
  }
}

public char operator [](string* s, int index) {
  embed {
    return (*s)[index];
  }
}

public string copy(string* s) {
  return *s;
}

