// no_compile

union V {
  int x;
  string s;
};

void send(V) {}

int main() {
  mutable v = V::x(3);
  mutable res = 0;
  mutable resS = "";
  switch (v) {
    case (mutable int x) {
      res = move(x);
    }
    case (mutable string s) {
      resS = move(s);
    }
  }
  send(move(v));
  return res;
}
